{
    "name": "digifactory/laravel-partial-down",
    "description": "Put specific parts of your application in maintenance mode using",
    "keywords": [
        "digifactory",
        "partial-down"
    ],
    "homepage": "https://github.com/digifactory/laravel-partial-down",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mark Jansen",
            "email": "mark@digifactory.nl",
            "role": "Developer"
        }
    ],
    "require": {
         "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.12",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DigiFactory\\PartialDown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigiFactory\\PartialDown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigiFactory\\PartialDown\\PartialDownServiceProvider"
            ]
        }
    }
}
